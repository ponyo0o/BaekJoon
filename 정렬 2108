from collections import Counter
import sys

n=int(sys.stdin.readline())

li=[]
for i in range(n):
    li.append(int(sys.stdin.readline()))

avg=sum(li)/n   #산술평균
print("%.0f"%avg)  # = print(round(avg))


li.sort()       #중앙값
print(li[n//2])

li.sort()          
c_li=Counter(li).most_common(2)      #최빈값
if len(li)>1:
    if c_li[0][1]==c_li[1][1]:
        print(c_li[1][0])
    else:
        print(c_li[0][0])
else:
    print(li[0])

print(max(li)-min(li))   #범위
 
