import sys       #방법1

n=int(sys.stdin.readline())

li=[]
for i in range(n):
    a,b=sys.stdin.readline().strip().split()
    li.append((int(a),b))
c=sorted(li,key=lambda x:x[0])

for i in c:
    print(i[0],i[1])


